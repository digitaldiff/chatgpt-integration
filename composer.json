{
    "name": "digitaldiff/craft-chatgpt-integration",
    "description": "Integration for ChatGPT",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "support@diff.ch"
    },
    "authors": [
        {
            "name": "diff. Kommunikation AG",
            "homepage": "https://www.diff.ch/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "craftcms/cms": "^5.0.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "publishing\\chatgptintegration\\": "src/"
        }
    },
    "extra": {
        "handle": "chatgpt-integration",
        "name": "ChatGPT Integration",
        "developer": "diff. Kommunikation AG",
        "documentationUrl": ""
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

